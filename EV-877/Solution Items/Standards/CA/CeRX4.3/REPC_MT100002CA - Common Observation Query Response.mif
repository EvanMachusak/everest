   <mif:containedClass>
      <mif:class graphicLinkId="771DBD34-F8D9-A3C8-F3D7-628A15502484" name="Subject2"
                 isAbstract="false">
         <mif:businessName name="Notes Indicator"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">If true, indicates that there are notes associated with the common observation.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Shows whether additional information is available on further drill-down.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">This association is mandatory as it indicates one of presence or absence notes.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="SUBJ"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
