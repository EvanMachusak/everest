                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Enables communication for verification of the measurement, and is therefore mandatory.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
