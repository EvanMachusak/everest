         <mif:businessName name="caused by"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates the event/process that caused the issue to occur.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">The issue can be caused by information on the Patient’s profile, including Active Medication and/or Dispense, and/or Patient Coded Observations (pregnancy, geriatric, allergy/intolerance, etc), and/or Patient Measurable Observations (height, weight, etc)</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Helps with management of the issue.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="SUBJ"
                        isImmutable="true">
