   <mif:containedClass>
      <mif:class graphicLinkId="28892700-6144-96DB-C33B-1EF9050B3A68" name="Custodian2"
                 isAbstract="false">
         <mif:businessName name="*d:associated with"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Participation"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>This indicates the physical facility where the Health Condition record is stored. This is generally the same as the location where the action was performed, but in some cases may be different. (E.g. where the action was done at the patient's home, but the data is captured and stored in the clinician's clinic.</html:i>
                     </html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>Used to indicate where records are likely kept for follow-up. This must always be known and is therefore mandatory. It may also be useful in understanding the context in which the event occurred.</html:i>
                     </html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="CST"
                        isImmutable="true">
