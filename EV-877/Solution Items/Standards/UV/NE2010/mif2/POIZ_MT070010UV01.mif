                   className="ParameterList">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>Entry point for Patient Immunization Status Query message. The intent of this query is to retrieve the complete immunization recommendation and current immunization status for a specific patient.</mif:text>
            </mif:description>
         </mif:documentation>
      </mif:annotations>
   </mif:entryPoint>
