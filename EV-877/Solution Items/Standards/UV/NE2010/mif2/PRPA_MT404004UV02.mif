            <mif:derivedFrom staticModelDerivationId="3" className="HomeHealthEncounterEvent"
                             attributeName="effectiveTime"/>
            <mif:type name="IVL">
               <mif:argumentDatatype name="TS"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute name="activityTime" sortKey="7" minimumMultiplicity="0" maximumMultiplicity="1"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The total time interval for this encounter including preparation before the patient arrives and clean-up actions after the patient departs</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="PatientEncounter"
                             attributeName="activityTime"/>
            <mif:derivedFrom staticModelDerivationId="2" className="HomeHealthEncounterEvent"
                             attributeName="activityTime"/>
            <mif:derivedFrom staticModelDerivationId="3" className="HomeHealthEncounterEvent"
                             attributeName="activityTime"/>
            <mif:type name="IVL">
               <mif:argumentDatatype name="TS"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute name="lengthOfStayQuantity" sortKey="8" minimumMultiplicity="0"
                        maximumMultiplicity="1"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The actual quantity of time for the patient encounter.The actual quantity cannot be simply calculated from the start and end dates because of possible leaves of absence.</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="PatientEncounter"
                             attributeName="lengthOfStayQuantity"/>
            <mif:derivedFrom staticModelDerivationId="2" className="HomeHealthEncounterEvent"
                             attributeName="lengthOfStayQuantity"/>
            <mif:derivedFrom staticModelDerivationId="3" className="HomeHealthEncounterEvent"
                             attributeName="lengthOfStayQuantity"/>
            <mif:type name="PQ"/>
         </mif:attribute>
         <mif:attribute name="dischargeDispositionCode" sortKey="9" minimumMultiplicity="0"
                        maximumMultiplicity="1"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>A code depicting the disposition of the patient at the end of the encounter (e.g., discharged to home, expired, against medical advice, etc.). Note: in the US realm this references the UB92 FL 22 - Patient Status value set.</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="PatientEncounter"
                             attributeName="dischargeDispositionCode"/>
            <mif:derivedFrom staticModelDerivationId="2" className="HomeHealthEncounterEvent"
