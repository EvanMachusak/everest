   <mif:containedClass>
      <mif:class graphicLinkId="A6465DD2-65B2-B135-BD1D-ECE305FEBBF2" name="Subject11"
                 isAbstract="false">
         <mif:businessName name="*has history"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Shows how the status of the active medication has changed over time.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Allows providers to understand what the status of a particular active medication was on a given date. Historical statuses may impact views of compliance.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
