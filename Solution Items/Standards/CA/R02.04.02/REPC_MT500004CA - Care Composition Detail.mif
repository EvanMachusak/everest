            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActMood" code="EVN"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="3" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:businessName name="G:Replaced by Record Id"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Used to identify the record that supersedes the current record. This attribute is set when a new record identifies the current record as being replaced.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Used in circumstances where a newer or corrected version of the record of this event exists.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="id"/>
            <mif:type name="II.BUS"/>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="EDCDAE3B-49FF-79AE-D197-3AFAB78B6999"
                 name="OldPatientCareProvisionEvent"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="Act"/>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PCPR"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActClass" code="PCPR"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="moodCode" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="EVN"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="moodCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActMood" code="EVN"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="3" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:businessName name="F:Replaces Record Ids"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Used to identify any records that are "superseded" by the current record. This will cause the referenced records to be marked as "obsolete" with a reference pointing to this record.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Used in circumstances where information initially captured about an event is erroneous, incomplete or not captured at the desired level of detail and the change cannot be made by retracting the original record. (E.g. Too much time has elapsed, change is being made by a provider other than the original author of the vent record, etc.) May also be used to reference multiple records in the case where the same event has been accidentally captured more than once.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="id"/>
            <mif:type name="II.BUS"/>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="0C6F104C-6052-364F-0EDA-948123B27C77" name="Outcome"
                 isAbstract="false">
         <mif:businessName name="o:resulted in"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Identifies any health conditions that have been newly identified in the patient as part of the encounter or which have been implicated as contributing to the need for the encounter. Discharge diagnosis represents "what is known or understood" as a result of the activities and observations that have taken place during the encounter.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">These are key search criteria for encounters and provide a good high-level understanding of the activity within an encounter without needing to look at the details. However, they are only 'required' because discharge diagnosis are not gathered for every encounter.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
            <mif:appInfo>
               <mif:formalConstraint graphicLinkId="549E941B-1B7E-976B-7801-47DFDA59EDC8">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">May only be used when Care Composition Type is Encounter or a specialization of Encounter</html:p>
                  </mif:text>
               </mif:formalConstraint>
            </mif:appInfo>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="OUTC"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActRelationshipType" code="OUTC"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="contextControlCode" sortKey="2" minimumMultiplicity="1"
                        maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="AN"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="contextControlCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ContextControl" code="AN"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="contextConductionInd" sortKey="3" minimumMultiplicity="1"
                        maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="true"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="contextConductionInd"/>
            <mif:type name="BL"/>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="C540DD73-75C0-9F1D-315A-5A43D4F34B41" name="Participant"
                 isAbstract="false">
         <mif:businessName name="*has"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Participation"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Identifies the participants associated with the particular location.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Important for providing context for the encounter. The element is mandatory because there whole purpose of having a participant grouping is to have participants.</html:p>
                  </mif:text>
               </mif:rationale>
               <mif:designComments>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Cardinality is constrained to 200 rather than 1000 because it is exactly equal to the sum of the number of potential patients and providers</html:p>
                  </mif:text>
               </mif:designComments>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PART"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Participation" attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ParticipationType" code="PART"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="modeCode" sortKey="2" minimumMultiplicity="0" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="B:Participation Mechanism"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Identifies how the patient and/or provider(s) participated in the encounter (in person, by phone, by videoconference, etc.)</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Understanding how each participant was involved in the encounter gives context to the encounter, particularly for telehealth.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Participation" attributeName="modeCode"/>
            <mif:type name="CV"/>
            <mif:vocabulary>
               <mif:conceptDomain name="ParticipationMode"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="000CAA8B-97BA-5404-E468-6566E313F898" name="Patient"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="Patient"/>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PAT"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Patient" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="RoleClass" code="PAT"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="95ED887C-9566-A947-FF84-9B2C15453C96"
                 name="PatientCareProvisionEvent"
                 isAbstract="false">
         <mif:businessName name="Care Composition"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Act"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">A care composition is a record with two purposes. It indicates that care of a given type has occurred or is occurring. It also acts as a collector for the events that happened during care, including who is responsible for the care provided.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Care composition messages may be sent during the course of care to describe the progress of care or may be sent at the termination of care to describe all the activities that occurred during the provision of care. Note that this record merely captures the existence of care and the locations and people involved. The actual discrete events and any care summary documentation that results are captured using other messages.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Examples include: encounters, condition-related care (episodes) and longer-term care collections such as "gynecological care".</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Care compositions allow grouping together numerous related records which aids searching and navigation. Also, the mere knowledge that a type of care has occurred or is occurring (e.g. an in-patient hospital encounter) can be useful information when delivering subsequent care.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
            <mif:appInfo>
               <mif:formalConstraint graphicLinkId="1BE29C24-B7C4-4A2D-0172-D7880A13D920">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Annotation is only permitted if Annotation Indicator is not present and vice versa</html:p>
                  </mif:text>
               </mif:formalConstraint>
            </mif:appInfo>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PCPR"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActClass" code="PCPR"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="moodCode" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="EVN"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="moodCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActMood" code="EVN"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="3" minimumMultiplicity="2" maximumMultiplicity="2"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:businessName name="A: Care Composition Ids"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>A globally unique identifier assigned by the EHR to the Care Composition record.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Allows for unique identification of the Care Composition and is therefore mandatory. Supports drill-down queries, linking of this record to other records, matching of EHR records to locally-stored PoS records and is necessary when identifying records for amending (revising)/directional linking (superseding).</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="id"/>
            <mif:type name="SET">
               <mif:argumentDatatype name="II.BUS_AND_VER"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute name="code" sortKey="4" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:businessName name="B: Care Composition Type"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Identifies the type of Care Composition represented by this record.</html:i>
                        </html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Care Composition is the generic name given to event 'containers' such as Encounters, Health-Condition-based Collections (Episodes) and Care-based Collections. The "type" of care composition places constraints on what elements are supported.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Encounter-based collection: a series of health care events that occur during an interaction between one or more health care providers and one or more patients where the providers and the patient remain in the same location over a contiguous period of time; the providers and patient may be at different locations (telehealth). e.g. Inpatient encounter, Community encounter.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Condition-based collection (Episode): a series of interactions between a patient and one or more health care providers over time in one or more locations and tied together by a common diagnosis or problem.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Care-based collection: a high level grouping of health events, encounters, and/or episodes related to a particular area of healthcare. e.g. Gynecology care, Cardiology Care, etc.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Care Composition Type is used for searching and for organizing Care Composition records as well as sorting them for presentation.</html:i>
                        </html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>This is a key attribute for understanding the type of record and is therefore mandatory.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="code"/>
            <mif:type name="CV"/>
            <mif:vocabulary>
               <mif:conceptDomain name="ActCareEventType"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="negationInd" sortKey="5" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        defaultValue="false"
                        isImmutable="true">
            <mif:businessName name="D:Refuted Indicator"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>When set to true, specifically flags the Care Composition record as "did not occur". The default is false. Additional details about the reasons for refuting the record may be conveyed in notes.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>This is primarily used to supersede records where an assertion was made that is subsequently determined to be false. It is important to be able to make explicit statements that something is known to not be true.</html:i>
                        </html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>This element is mandatory because it should always be known whether the record is being refuted or not.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
                  <mif:formalConstraint graphicLinkId="D7AD9D58-0853-7960-192C-26D87A30CE40">
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Refuted Indicator cannot be specified unless Care Composition Type is Encounter or a specialization there-of.</html:p>
                     </mif:text>
                  </mif:formalConstraint>
               </mif:appInfo>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="negationInd"/>
            <mif:type name="BL"/>
         </mif:attribute>
         <mif:attribute name="statusCode" sortKey="6" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="C: Care Composition Status"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>This identifies the current state of the Care Composition record. Allowed status values are 'active' (the encounter, episode or general delivery of care ongoing) and 'completed' (the encounter, episode or general care has ended).</html:i>
                        </html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Status is frequently used to filter query responses as well as to sort records for presentation. It also affects how the Care Composition record is interpreted.</html:i>
                        </html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Because the status won't always be known, the attribute is marked as 'populated' to allow the use of null flavors.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="statusCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:conceptDomain name="ActStatus"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="effectiveTime" sortKey="7" minimumMultiplicity="1"
                        maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="F: Care Composition Period"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Represents the start and end of the date/time interval during which the care described by the composition was/is being provided.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">E.g. The admission and discharge date/time; the date on which the episode began and ended; etc.</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Please note that it is possible for many episodes and care events to not have an end date; in these situations, only the start date will be specified. Even encounters will not have an end date until the encounter is completed/patient is discharged.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Identifies the time-period of relevance to the record that is useful in filtering and organizing "time-view" presentations of data. Because the timing information won't always be known, this attribute is marked as 'populated'.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="effectiveTime"/>
            <mif:type name="IVL">
               <mif:argumentDatatype name="TS.DATETIME"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute name="confidentialityCode" sortKey="8" minimumMultiplicity="0"
                        maximumMultiplicity="2"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="E: Care Composition Masking Indicators"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Communicates the desire of the patient to restrict access to this Care Composition record. Provides support for additional confidentiality constraint, giving patients a level of control over their information. Methods for accessing masked event records will be governed by each jurisdiction (e.g. court orders, shared secret/consent, etc.).</html:i>
                        </html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Can also be used to communicate that the information is deemed to be sensitive and should not be communicated or exposed to the patient (at least without the guidance of the authoring or other responsible healthcare provider).</html:i>
                        </html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>Valid values are: 'normal' (denotes 'Not Masked'); 'restricted' (denotes 'Masked') and 'taboo' (denotes 'patient restricted'). The default is 'normal' signifying 'Not Masked'. Either or both of the other codes can be asserted to indicate masking by the patient from providers or masking by a provider from the patient, respectively. 'normal' should never be asserted with one of the other codes.</html:i>
                        </html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Masking a care composition record masks it for all associated patients (i.e. all patients involved in the care composition as a group).</html:p>
                     
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Also, masking a care composition implicitly masks all records associated with that care composition.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                           <html:i>The value specified for a particular record may be overridden by a higher level masking applied to an indication, a care composition, a type of record or even all patient records.</html:i>
                        </html:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml"/>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="confidentialityCode"/>
            <mif:type name="SET">
               <mif:argumentDatatype name="CV"/>
            </mif:type>
            <mif:vocabulary>
               <mif:conceptDomain name="x_BasicConfidentialityKind"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="C15047A6-6E6A-2064-4816-0FCFF4CC640B"
                 name="PatientCareProvisionEventPortion"
                 isAbstract="false">
         <mif:businessName name="Participant Groupings"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Act"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Identifies a 'portion' of the encounter performed at a single location.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Allows tracking what participants were present at each location within the encounter. This information provides context for the events within the encounter.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
            <mif:appInfo>
               <mif:formalConstraint graphicLinkId="1B3CEA84-2F14-01A1-17BF-7F54A26E9E73">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">May only be used when Care Composition Type is Encounter or a specialization of Encounter</html:p>
                  </mif:text>
               </mif:formalConstraint>
               <mif:formalConstraint graphicLinkId="BF174168-233D-8392-B7DF-4FA15D41A97E">
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">All participant identifiers must correspond to either patient identifiers or performer identifiers</html:p>
                  </mif:text>
               </mif:formalConstraint>
            </mif:appInfo>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PCPR"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActClass" code="PCPR"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="moodCode" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="EVN"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="moodCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActMood" code="EVN"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="707E491E-7B21-EF45-E2EE-AD68F617F59D" name="Performer3"
                 isAbstract="false">
         <mif:businessName name="d:performed by"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Participation"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>Identifies the person who actually performed the action described in the record.</html:i>
                     </html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For outpatient encounters, identifies the provider(s) with principle responsibility for the encounter. For inpatient/institutional encounters, this generally isn't used (admitting and discharge providers are captured elsewhere).</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">For condition-based (episode) collections and care-based collections, identifies the provider(s) currently responsible for managing the episode or type of care (may change over time) described by this record.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">NOTE: This is *not* intended to capture all providers involved in care, merely those responsible for care. In most circumstances this will only be one provider. However, in some circumstances (e.g. telehealth) it may be necessary to capture more than one.</html:p>
                  
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">In circumstances where number of providers involved is greater than the limit supported by the message, multiple encounters should be used.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:usageNotes>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>If not specified, it is assumed that the author is also the performer. I.e. If the author and performer are the same, only the author should be sent. If the author is not the performer, but the performer is not known, a null flavor of UNK (unknown) should be used.</html:i>
                     </html:p>
                  </mif:text>
               </mif:usageNotes>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">
                        <html:i>The person who makes the decision to capture information in the electronic record is not always the person who actually performed the action. Knowledge of the performer is useful both for tracking responsibility and for subsequent follow-up. Multiple repetitions are allowed for those circumstances where more than one provider is involved.</html:i>
                     </html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="PRF"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Participation" attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ParticipationType" code="PRF"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="contextControlCode" sortKey="2" minimumMultiplicity="1"
                        maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="OP"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Participation"
                             attributeName="contextControlCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ContextControl" code="OP"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="4E08D4CD-C906-568F-B0F2-B467E8A28074" name="Predecessor"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="SUCC"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="typeCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ActRelationshipType" code="SUCC"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="contextControlCode" sortKey="2" minimumMultiplicity="1"
                        maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="ON"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="contextControlCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="ContextControl" code="ON"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="contextConductionInd" sortKey="3" minimumMultiplicity="1"
                        maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="false"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"
                             attributeName="contextConductionInd"/>
            <mif:type name="BL"/>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="3D1F0FAC-D01D-4C47-F83D-607F1B9CE6E6" name="Predecessor2"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
