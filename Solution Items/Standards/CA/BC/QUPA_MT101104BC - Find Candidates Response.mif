            <mif:derivedFrom staticModelDerivationId="3" className="IdentifiedPerson"
                             attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="RoleClass" code="IDENT"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="2" minimumMultiplicity="0" maximumMultiplicity="*"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:businessName name="Client Healthcare Identification Number"/>
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <xhtml:p>This identification attribute supports capture of a healthcare identifier specific to the client. This identifier may be assigned jurisdictionally or by care facility.</xhtml:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <xhtml:p>PW - @extension maps to 4. EMPI.Source ID. Note that this could be multiple, if the querying organization has read access to several sources' identifiers. Also, if there are no source IDs (just a PHN) then there will be no instances here. Changed cardinality from 1..* to 0..*. @assigningAuthorityName maps to Source System ID.</xhtml:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:rationale>
                     <mif:text>
                        <xhtml:p>Mandatory attribute supports unique identification of the client.</xhtml:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
                  <mif:openIssue>
                     <mif:text>
                        <xhtml:p>bc 24-nov-10: BC specification has II datatype with 0..* cardinality. HL7 datatypes don't allow this combination, except as SET&lt;II&gt;.</xhtml:p>
                     </mif:text>
                  </mif:openIssue>
               </mif:appInfo>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="id"/>
            <mif:derivedFrom staticModelDerivationId="2" className="IdentifiedPerson" attributeName="id"/>
            <mif:derivedFrom staticModelDerivationId="3" className="IdentifiedPerson" attributeName="id"/>
            <mif:type name="SET">
               <mif:argumentDatatype name="II"/>
            </mif:type>
         </mif:attribute>
         <mif:attribute name="addr" sortKey="3" minimumMultiplicity="0" maximumMultiplicity="2"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text/>
                  </mif:definition>
                  <mif:designComments>
                     <mif:text>
                        <xhtml:p>bc 24-nov-10: attributed added to model, per BC spec.</xhtml:p>
                     </mif:text>
                  </mif:designComments>
               </mif:documentation>
               <mif:appInfo>
                  <mif:openIssue>
                     <mif:text>
                        <xhtml:p>bc 24-nov-10: BC datatype is simple AD, with 0..2 cardinality. Tooling requires use of BAG&lt;AD&gt;.</xhtml:p>
                     </mif:text>
                  </mif:openIssue>
               </mif:appInfo>
