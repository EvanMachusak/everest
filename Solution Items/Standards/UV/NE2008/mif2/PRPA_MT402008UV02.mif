            <derivedFrom attributeName="statusCode" className="EncounterEvent" staticModelDerivationId="3"/>
            <type name="CS"/>
            <supplierVocabulary codingStrength="CNE"><vocabularyDomain xmlns="" name="ActStatus"/></supplierVocabulary>
         </attribute>
         <attribute isImmutable="false" isMandatory="false" maximumMultiplicity="1" minimumMultiplicity="0" name="effectiveTime" sortKey="6">
            <annotations>
               
            <documentation><definition>
                  <text>
                     <xhtml:p>The time interval starting with the administrative onset of the encounter (e.g. admission, registration, patient arrival) and ending with the patient's departure (e.g. discharge). Note: for active encounters the end of the effectiveTime range is the anticipated end date-time.</xhtml:p>
                  </text>
               </definition></documentation></annotations>
            <derivedFrom attributeName="effectiveTime" className="Act" staticModelDerivationId="1"/>
            <derivedFrom attributeName="effectiveTime" className="EncounterEvent" staticModelDerivationId="2"/>
            <derivedFrom attributeName="effectiveTime" className="EncounterEvent" staticModelDerivationId="3"/>
