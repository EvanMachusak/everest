         </mif:attribute>
         <mif:attribute name="code" sortKey="4" minimumMultiplicity="0" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The Benefit code: The type of benefit is required be specified by a code from the x_ActBillableCode value set if known. For example, medical, chiropractic, dental care, social work, hospice, long term care, ambulance, acupuncture, pharmacy, psychiatry, naturopathy, physical therapy, etc.</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="code"/>
